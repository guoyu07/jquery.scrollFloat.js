var i=0,
	$delete = $('.action-delete .icon-delete'),
	deleteEleLength = $delete.length;

var clickDelete = function(){
	setTimeout(function(){
		$($('.btn-primary')[4]).trigger('click')
	},500)
}

var timer = setInterval(function(){
	var deleteEle = $delete[i];
	$(deleteEle).trigger('click');
	clickDelete();
	if(i==deleteEleLength){
		clearInterval(timer);
		alert('全删完了！！不给批量操作的PM不是好RD！')
	}
	i++;
},1000)